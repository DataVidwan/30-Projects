        "# Ask for user feedback\n",
        "feedback = input(\"Please enter your feedback: \")\n",
        "\n",
        "# Open a file in append mode\n",
        "with open(\"user_feedback.txt\", \"a\") as file:\n",
        "    file.write(feedback + \"\\n\")\n",
        "\n",
        "print(\"✅ Your feedback has been saved! Thank you.\")\n",
        "\n"
