        "# Multiplication Table\n",
        "def multiplication_table():\n",
        "    num = int(input(\"Enter a number for the multiplication table: \"))\n",
        "    for i in range(1, 11):\n",
        "        print(f\"{num} x {i} = {num * i}\")\n",
        "\n",
        "multiplication_table()\n"
      ]
